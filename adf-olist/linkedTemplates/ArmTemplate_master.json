{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-olist"
		},
		"ls_databricks_olist_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks_olist'"
		},
		"ls_adls_olist_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stolistlakehousene.dfs.core.windows.net"
		},
		"ls_adls_olistne_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stolistlakehousene.dfs.core.windows.net/"
		},
		"ls_databricks_olist_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1205-195430-mtytj2jr"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-olist_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_databricks_olist_accessToken": {
						"value": "[parameters('ls_databricks_olist_accessToken')]"
					},
					"ls_adls_olist_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_olist_properties_typeProperties_url')]"
					},
					"ls_adls_olistne_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_olistne_properties_typeProperties_url')]"
					},
					"ls_databricks_olist_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_olist_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}