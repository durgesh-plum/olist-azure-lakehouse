{
	"name": "ds_bronze_sink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_adls_olist",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileSystem": {
				"type": "string",
				"defaultValue": "olist"
			},
			"Directory": {
				"type": "string",
				"defaultValue": "bronze"
			},
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(\n  dataset().Directory,\n  '/',\n  dataset().SubDirectory,\n  '/ingest_date=',\n  formatDateTime(utcNow(), 'yyyy-MM-dd')\n)\n",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().FileSystem\n",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}